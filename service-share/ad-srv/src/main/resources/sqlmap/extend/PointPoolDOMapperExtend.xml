<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lawu.eshop.ad.srv.mapper.extend.PointPoolDOMapperExtend">

  <select id="selectMember" parameterType="java.lang.Long" resultType="java.lang.Long">
      select member_id from point_pool WHERE ad_id=#{adId,jdbcType=BIGINT} and status=1 ORDER BY point desc  LIMIT 3
  </select>
 
</mapper>